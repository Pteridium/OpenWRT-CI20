&ext {
	clock-frequency = <48000000>;
};

&msc0 {
	bus-width = <4>;
	max-frequency = <48000000>;
	cd-gpios = <&gpf 20 GPIO_ACTIVE_LOW>;

	pinctrl-names = "default";
	pinctrl-0 = <&pins_msc0_pe>;
};

